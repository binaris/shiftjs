{
  "name": "shift-cli",
  "description": "ShiftJS CLI Tool",
  "version": "0.0.0",
  "author": "arikmaor @arikmaor",
  "bin": {
    "shift-cli": "./bin/run"
  },
  "bugs": "https://github.com/binaris/shiftjs/issues",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.2",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/plugin-not-found": "^1.2.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/tslint": "^3.1.1",
    "@types/node": "^10.14.14",
    "globby": "^10.0.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/binaris/shiftjs",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "shift-cli",
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/cli/<%- commandPath %>",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "binaris/shiftjs",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -c ../common/tslint.yml -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "lint": "tslint -c ../common/tslint.yml -p .",
    "test": "echo NO TESTS",
    "build": "npm run prepack",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
