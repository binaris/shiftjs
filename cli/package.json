{
  "name": "shift-cli",
  "description": "ShiftJS CLI Tool",
  "version": "0.1.2",
  "author": "arikmaor @arikmaor",
  "bin": {
    "shift-cli": "./bin/run"
  },
  "bugs": "https://github.com/binaris/shiftjs/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/binaris/shiftjs",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "shift-cli",
    "repositoryPrefix": "<%- repo %>/blob/shift-cli_v<%- version %>/cli/<%- commandPath %>",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "binaris/shiftjs",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -c ../common/tslint.yml -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "lint": "tslint -c ../common/tslint.yml -p .",
    "test": "echo NO TESTS",
    "build": "npm run prepack",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@binaris/spice-node-client": "0.0.2-rc.6",
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.2",
    "@oclif/errors": "^1.2.2",
    "@oclif/parser": "^3.8.4",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/plugin-not-found": "^1.2.2",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.1",
    "conf": "^5.0.0",
    "dotenv": "^8.0.0",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "ms": "^2.1.2",
    "mz": "^2.7.0",
    "node-fetch": "^2.6.0",
    "open": "^6.4.0",
    "tar": "^4.4.10",
    "terminal-link": "^1.3.0",
    "tslib": "^1.10.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/tslint": "^3.1.1",
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^8.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/ms": "^0.7.30",
    "@types/mz": "^0.0.32",
    "@types/node": "^10.14.14",
    "@types/node-fetch": "^2.5.0",
    "@types/tar": "^4.0.3",
    "@types/validator": "^10.11.2",
    "globby": "^10.0.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
