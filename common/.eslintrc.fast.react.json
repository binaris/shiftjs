{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "no-only-tests"
  ],
  "extends": [
    "./.eslintrc.fast.json",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prop-types": "off",
    "react/display-name": "off"
  },
  "settings": {
    "react": {
        "version": "detect"
    }
  }
}
